{
  "name": "melt-cover-cloud-shop",
  "version": "1.0.1",
  "main": "index.js",
  "license": "MIT",
  "sideEffects": [
    "*.css",
    "*.less",
    "@babel/polyfill",
    "reflect-metadata"
  ],
  "scripts": {
    "start": "fch start",
    "build": "rimraf dist && fch build",
    "build:dev": "rimraf dist && fch build -d -s",
    "build-ana": "rimraf dist && cross-env NODE_ENV=production IS_ANA=analyse webpack",
    "dll:dev": "rimraf dll/development && cross-env NODE_ENV=development webpack --config bundle/webpack.config.dll.js",
    "dll:prod": "rimraf dll/production && cross-env NODE_ENV=production webpack --config bundle/webpack.config.dll.js",
    "lint": "eslint src --ext .js,.ts",
    "lintcss": "stylelint 'src/**/*.less' --fix"
  },
  "dependencies": {
    "@ant-design/icons": "^4.2.1",
    "@fch/fch-shop-web": "0.0.25",
    "@fch/fch-tool": "0.0.8",
    "@sentry/browser": "^5.7.1",
    "@types/js-cookie": "^2.2.2",
    "@types/lodash": "^4.14.144",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.5",
    "@types/viewerjs": "^1.0.2",
    "antd": "~4.5.0",
    "axios": "^0.19.0",
    "compressorjs": "^1.0.6",
    "js-cookie": "^2.2.1",
    "lodash": "^4.17.15",
    "moment": "^2.29.1",
    "qiankun": "^2.0.11",
    "querystring": "^0.2.1",
    "react": "^16.13.1",
    "react-contexify": "4.1.1",
    "react-dom": "^16.13.1",
    "react-images": "^1.1.7",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.3",
    "viewerjs": "^1.10.0"
  },
  "devDependencies": {
    "@hot-loader/react-dom": "^16.11.0",
    "@svgr/webpack": "^4.3.2",
    "@teamsupercell/typings-for-css-modules-loader": "^2.0.0",
    "@types/ali-oss": "^6.0.10",
    "@typescript-eslint/eslint-plugin": "^2.3.1",
    "@typescript-eslint/parser": "^2.3.1",
    "add-asset-html-webpack-plugin": "^3.1.3",
    "ali-oss": "^6.16.0",
    "autoprefixer": "^9.6.1",
    "cache-loader": "^4.1.0",
    "copy-webpack-plugin": "^5.0.4",
    "cross-env": "^6.0.0",
    "css-loader": "^3.2.0",
    "dotenv": "~8.1.0",
    "dotenv-webpack": "~1.7.0",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-loader": "^3.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.3.0",
    "file-loader": "^4.2.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "lint-staged": "^9.3.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.18.2",
    "react-hot-loader": "^4.12.13",
    "rimraf": "^3.0.0",
    "ts-import-plugin": "^1.6.1",
    "ts-loader": "^6.1.2",
    "typescript": "^3.6.3",
    "url-loader": "^2.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{jsx,js,tsx,ts}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  }
}
